#!/bin/sh

#DEBHELPER#

if [ -e /etc/weatherstation/chart.conf ]; then
	LINKNAME="/usr/share/weatherstation-frontend/$(grep default_chart_dir /etc/weatherstation/frontend.conf | cut -f 2 -d "'" | cut -f 2 -d '"')"
	if [ -e $LINKNAME ]; then
		echo -n "Lösche Bilderlink ... "
		/bin/rm -f $LINKNAME
		echo "done"
	fi
fi
